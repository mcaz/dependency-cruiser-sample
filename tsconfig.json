{
  "compilerOptions": {
    "target": "ES2020",
    "useDefineForClassFields": true,
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ESNext",
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "allowJs": false,
    "noEmit": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "strict": false,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "src/*"
      ],
      "~components/*": [
        "src/components/*"
      ]
    }
  },
  "include": [
    "src"
  ]
}