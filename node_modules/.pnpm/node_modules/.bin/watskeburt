#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/cazma/Downloads/atomic-react-router-depcruise/node_modules/.pnpm/watskeburt@4.2.3/node_modules/watskeburt/dist/node_modules:/Users/cazma/Downloads/atomic-react-router-depcruise/node_modules/.pnpm/watskeburt@4.2.3/node_modules/watskeburt/node_modules:/Users/cazma/Downloads/atomic-react-router-depcruise/node_modules/.pnpm/watskeburt@4.2.3/node_modules:/Users/cazma/Downloads/atomic-react-router-depcruise/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/cazma/Downloads/atomic-react-router-depcruise/node_modules/.pnpm/watskeburt@4.2.3/node_modules/watskeburt/dist/node_modules:/Users/cazma/Downloads/atomic-react-router-depcruise/node_modules/.pnpm/watskeburt@4.2.3/node_modules/watskeburt/node_modules:/Users/cazma/Downloads/atomic-react-router-depcruise/node_modules/.pnpm/watskeburt@4.2.3/node_modules:/Users/cazma/Downloads/atomic-react-router-depcruise/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../watskeburt/dist/run-cli.js" "$@"
else
  exec node  "$basedir/../watskeburt/dist/run-cli.js" "$@"
fi
